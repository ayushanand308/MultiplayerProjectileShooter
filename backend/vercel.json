{
    "version": 2,
    "builds": [
      {
        "src": "./backend/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "./frontend/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/index.html"
      }
    ]
  }